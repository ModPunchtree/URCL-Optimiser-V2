
uint ack(uint m, uint n) {
    if (m == 0) {
        return n+1;
    } else if (n == 0) {
        return ack(m-1, 1);
    } else {
        return ack(m-1, ack(m, n-1));
    }
}

void printStr(char *p) {
    char *index = 0;

    while (*(p+index) != 0) {
        out(%text, *(p+index));
        index += 1;
    }
}

void main() {
    char string = {"Ack(6, 9) = ", 0};
    printStr(string);

    out(%numb, ack(6, 9));
}



